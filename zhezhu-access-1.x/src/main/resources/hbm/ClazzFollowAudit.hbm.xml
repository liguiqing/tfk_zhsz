<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field" default-cascade="all" >

    <class name="com.tfk.access.domain.model.school.ClazzFollowAudit" table="ac_ClazzFollowAudit" lazy="true">
        <id name="id" type="long" column="id" unsaved-value="-1">
            <generator class="native" />
        </id>
        <component name="auditId" class="com.tfk.share.domain.id.access.ClazzFollowAuditId">
            <property name="id" column="auditId" type="java.lang.String"/>
        </component>
        <component name="applyId" class="com.tfk.share.domain.id.access.ClazzFollowApplyId">
            <property name="id" column="applyId" type="java.lang.String"/>
        </component>

        <component name="auditorId" class="com.tfk.share.domain.id.PersonId">
            <property name="id" column="auditorId" type="java.lang.String"/>
        </component>
        <property name="ok" column="ok" type="java.lang.Boolean"/>
        <property name="auditDate" column="auditDate" type="java.util.Date"/>
        <property name="description" column="description" type="java.lang.String"/>
    </class>

</hibernate-mapping>

