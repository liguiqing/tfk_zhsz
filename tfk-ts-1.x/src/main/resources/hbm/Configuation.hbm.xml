<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field" default-cascade="all" >
    <class name="com.tfk.ts.domain.model.school.common.SchoolConfig" table="ts_configuation" lazy="true">
        <id name="id" type="long" column="id" unsaved-value="-1">
            <generator class="native" />
        </id>
        <component name="schoolId" class="com.tfk.ts.domain.model.school.SchoolId">
            <property name="id" column="schoolId" type="java.lang.String"/>
        </component>
        <component name="configuation" class="com.tfk.ts.domain.model.school.common.Configuation">
            <property name="name" column="name" type="java.lang.String"/>
            <property name="value" column="value" type="java.lang.String"/>
            <property name="description" column="description" type="java.lang.String"/>
        </component>
    </class>
</hibernate-mapping>