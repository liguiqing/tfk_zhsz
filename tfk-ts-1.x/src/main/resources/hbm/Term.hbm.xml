<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field" default-cascade="all" >
    <class name="com.tfk.ts.domain.model.school.term.Term" table="ts_term" lazy="true">
        <id name="id" type="long" column="id" unsaved-value="-1">
            <generator class="native" />
        </id>
        <component name="termId" class="com.tfk.ts.domain.model.school.term.TermId">
            <property name="id" column="termId" type="java.lang.String"/>
        </component>
        <property name="name" column="name" type="java.lang.String"/>
        <component name="termYear" class="com.tfk.ts.domain.model.school.StudyYear">
            <property name="name" column="yearName" type="java.lang.String" />
            <property name="startsYear" column="startsYear" type="java.lang.Integer"/>
            <property name="endsYear" column="endsYear" type="java.lang.Integer"/>
        </component>
        <property name="order" column="seq" >
            <type name="org.hibernate.type.EnumType">
                <param name="enumClass">com.tfk.ts.domain.model.school.term.TermOrder</param>
                <param name="usedName">true</param>
            </type>
        </property>
        <component name="timeSpan" class="com.tfk.ts.domain.model.school.term.TimeSpan">
            <property name="starts" column="starts" type="java.sql.Date"/>
            <property name="ends" column="ends" type="java.sql.Date"/>
        </component>
        <component name="schoolId" class="com.tfk.ts.domain.model.school.SchoolId">
            <property name="id" column="schoolId" type="java.lang.String"/>
        </component>
    </class>
</hibernate-mapping>
